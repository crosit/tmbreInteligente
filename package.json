{
  "name": "app_name",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "start": "NODE_ENV=production concurrently \"rimraf dist\" \"node dist/index.js\"",
    "clean": "concurrently \"rimraf dist\" \"rimraf tsconfig.tsbuildinfo\" \"tsc\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:create": "npm run typeorm -- migration:create -n",
    "migrations:generate": "npm run typeorm -- migration:generate -n",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "migrations:drop": "npm run typeorm -- migration:drop",
    "migrations:rollback": "npm run typeorm -- migration:revert"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.4.0",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "i": "^0.3.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.3.0",
    "node-fetch": "^3.0.0",
    "npm": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.6.5",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.2.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.13",
    "typescript": "^4.5.2"
  }
}
